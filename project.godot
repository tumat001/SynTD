; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://git_api.gdns"
} ]
_global_script_class_icons={
"GitAPI": ""
}

[application]

config/name="Roguelike Tower Defence"
run/main_scene="res://Maps/TestMap/TestMap.tscn"
config/icon="res://icon.png"

[autoload]

BulletMetadata="*res://TowerRelated/BulletMetadata.gd"
DamageType="res://GameInfoRelated/DamageType.gd"
Modifier="res://GameInfoRelated/Modifier.gd"
FlatModifier="res://GameInfoRelated/FlatModifier.gd"
PercentModifier="res://GameInfoRelated/PercentModifier.gd"
OnHitDamage="res://GameInfoRelated/OnHitDamage.gd"
EffectType="res://GameInfoRelated/EffectType.gd"
OnHitEffect="res://GameInfoRelated/OnHitEffect.gd"
PercentType="res://GameInfoRelated/PercentType.gd"
EnemyConstants="res://EnemyRelated/EnemyConstants.gd"

[debug]

shapes/collision/shape_color=Color( 0, 0.6, 0.701961, 0.501961 )

[display]

window/size/width=960
window/size/height=540
window/size/borderless=true
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

[layer_names]

2d_physics/layer_1="TowerBaseArea"
2d_physics/layer_2="TowerRangeArea"
2d_physics/layer_3="TowerBullet"
2d_physics/layer_4="TowerAura"
2d_physics/layer_6="EnemyGround"
2d_physics/layer_7="EnemyAir"
2d_physics/layer_8="EnemyBullets"
2d_physics/layer_9="EnemyAuraToAllies"
2d_physics/layer_10="EnemyAuraToTowers"

[rendering]

environment/default_environment="res://default_env.tres"
